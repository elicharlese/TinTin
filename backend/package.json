{
  "name": "tintin-backend",
  "version": "1.0.0",
  "description": "Backend services for TinTin personal finance application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t tintin-backend .",
    "docker:run": "docker run -p 3001:3001 tintin-backend",
    "migration:generate": "supabase gen types typescript --local > src/types/supabase.ts",
    "migration:up": "supabase db push",
    "migration:reset": "supabase db reset",
    "seed": "tsx src/scripts/seed.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.3.9",
    "@coral-xyz/anchor": "^0.29.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.4.0",
    "rate-limiter-flexible": "^4.0.1",
    "cron": "^3.1.6",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "node-cron": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "jest-extended": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@types/nodemailer": "^6.4.14",
    "@types/multer": "^1.4.11",
    "@types/joi": "^17.2.3",
    "@types/jest": "^29.5.8",
    "@types/pg": "^8.10.9",
    "@types/cron": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "fintech",
    "personal-finance",
    "solana",
    "blockchain",
    "supabase",
    "typescript"
  ],
  "author": "TinTin Team",
  "license": "MIT"
}
